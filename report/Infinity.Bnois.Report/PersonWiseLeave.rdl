>true</PrintOnFirstPage><PrintOnLastPage>true</PrintOnLastPage><Style><Border><Style>None</Style></Border></Style></PageFooter><LeftMargin>0.5in</LeftMargin><RightMargin>0.5in</RightMargin><TopMargin>0.5in</TopMargin><BottomMargin>0.5in</BottomMargin><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="DataSource1"><DataSourceReference>BnoisDataSource</DataSourceReference><rd:SecurityType>DataBase</rd:SecurityType><rd:DataSourceID>b8d07b5b-9207-446c-836e-51f8bed3c768</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="DataSet1"><Query><DataSourceName>DataSource1</DataSourceName><QueryParameters><QueryParameter Name="@EmployeeId"><Value>=Parameters!EmployeeId.Value</Value></QueryParameter></QueryParameters><CommandText>delete  temp.LeaveBreakDown where EmpId=@EmployeeId
Declare @LeaveType Table (
RowNumber int,
LeaveTypeId int,
CommissionTypeId int,
LeaveDuration int,
LeaveDurationType varchar(10),
ForeignDuration int,
ForeignDurationType varchar(10),
TmYear int,
YearText int )

insert into @LeaveType
  select ROW_NUMBER() OVER(ORDER BY CommissionTypeId) RowNumber,LeaveTypeId, CommissionTypeId,case when LeaveDurationType = 'M' then (LeaveDuration * 30) else LeaveDuration end LeaveDuration,LeaveDurationType,case when ForeignDurationType ='M'  then (ForeignDuration *  30) else ForeignDuration end ForeignDuration,ForeignDurationType,TmYear,0 YearText from LeavePolicy


Declare @YearInfo Table (
YearText int,
LeaveType int
)

Declare @YearWiseLeave Table(
YearText int,
DaysPerYear int
)
Declare @LeaveBalance Table (
EmpId int
      ,LeaveTypeId  int
      ,CommissionTypeId int
      ,LeaveDuration int
      ,LeaveDurationType varchar(10)
      ,ForeignDuration int
      ,ForeignDurationType varchar(10)
      ,TmYear int
      ,YearText int
      --,FormDate int
      --,ToDate int
      --,Duration int
      --,Slot int
)

DECLARE @counter int SET @counter = 1
--Declare @LeaveYear int = (select top(1) TmYear from LeavePolicy where LeaveTypeId=@LeaveTypeId )
DECLARE @CommissionDate datetime = (select CommissionDate from EmployeeGeneral where EmployeeId = @EmployeeId)
DECLARE @ServiceYear int =(select DATEPART(year, @CommissionDate))

--Declare @Slot int   =Isnull((select top(1) Slot from LeavePolicy where LeaveTypeId=@LeaveTypeId ), 0)
DECLARE @CYear int =YEAR(GETDATE())
------------ Year Wise Day Insert --------------

insert into @YearWiseLeave select year(dt) [YearText], count(Day(dt) ) [DaysPerYear]
from (select top(datediff(d, @CommissionDate, GETDATE())) dateadd(d,  row_number() over (order by (select null)), @CommissionDate) dt
      from sys.columns) q
group by year(dt)
order by [YearText], [DaysPerYear]
--------  Year add According to Leave Type Id ---------
while @counter &lt;= (SELECT COUNT(*) FROM @LeaveType)
BEGIN
WHILE @ServiceYear &lt;= @CYear
BEGIN
INSERT INTO	 @YearInfo select @ServiceYear YearText,(SELECT LeaveTypeId FROM @LeaveType where RowNumber =@counter) LeaveType


SET @ServiceYear=@ServiceYear + (SELECT TmYear FROM @LeaveType where RowNumber =@counter)
if( (SELECT TmYear FROM @LeaveType where RowNumber =@counter) = 0) BREAK
END
SET @counter = @counter + 1
SET @ServiceYear =(select DATEPART(year, @CommissionDate))
END

insert into @LeaveBalance
select @EmployeeId EmpId,a.LeaveType LeaveTypeId,b.CommissionTypeId ,Isnull((select Round(cast(b.LeaveDuration as float) / Cast (DATEDIFF(day,  cast(a.YearText as char(4)),  cast(a.YearText+1 as char(4))) as int) * h.DaysPerYear,0) from @YearWiseLeave h where h.YearText = a.YearText),b.LeaveDuration) LeaveDuration ,b.LeaveDurationType ,b.ForeignDuration ,b.ForeignDurationType ,b.TmYear,a.YearText  from @YearInfo a
inner join  @LeaveType b on b.LeaveTypeId = a.LeaveType


insert into temp.LeaveBreakDown
select a.*,bb.CreatedDate,bb.FromDate,bb.ToDate,bb.Duration,bb.Slot,bb.EmpLeaveId,'' from @LeaveBalance a
left join (  SELECT DISTINCT  ct.EmployeeId,ct.LeaveTypeId,ct.EmpLeaveId,ct.FromDate,ct.ToDate,ct.CreatedDate,ct.Duration,year(ct.CreatedDate) Year,
   STUFF(
      (SELECT ', ' + convert(varchar(50), c2.YearText)
      FROM EmployeeLeaveYear c2
      WHERE c1.EmpLeaveId = c2.EmpLeaveId 
	group by c2.YearText,c2.EmpLeaveId
      FOR XML PATH('') ), 1, 2, '') as Slot
FROM EmployeeLeaveYear c1  
JOIN EmployeeLeave ct ON c1.EmpLeaveId = ct.EmpLeaveId
group by c1.YearText,c1.EmpLeaveId,ct.EmpLeaveId,ct.FromDate,ct.ToDate,ct.CreatedDate,ct.Duration,ct.EmployeeId,ct.LeaveTypeId

) bb on bb.EmployeeId =a.EmpId and bb.LeaveTypeId=a.LeaveTypeId and bb.Year =a.YearText

update temp.LeaveBreakDown set Country =( select Country from (select 	c1.EmpLeaveId,   STUFF(
      (SELECT ', ' + convert(varchar(50),(select FullName from Country where CountryId =  c2.CountryId) )
      FROM EmployeeLeaveCountry c2	  
      WHERE c1.EmpLeaveId = c2.EmpLeaveId 
	group by c2.CountryId,c2.EmpLeaveId
      FOR XML PATH('') ), 1, 2, '') as Country
FROM EmployeeLeaveCountry c1  
JOIN EmployeeLeave ct ON c1.EmpLeaveId = ct.EmpLeaveId
group by c1.EmpLeaveId,ct.EmpLeaveId,ct.FromDate,ct.ToDate,ct.CreatedDate,ct.Duration,ct.EmployeeId,ct.LeaveTypeId) ss where temp.LeaveBreakDown.EmpLeaveId = ss.EmpLeaveId)

SELECT (select FullNameEng from Employee where EmployeeId = @EmployeeId)FullNameEng,(select TypeName from LeaveType where LeaveTypeId = temp.LeaveBreakDown.LeaveTypeId) LeaveType,temp.LeaveBreakDown.* FROM temp.LeaveBreakDown 
where EmpId=@EmployeeId and LeaveTypeId =3 and FormDate is not null</CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="FullNameEng"><DataField>FullNameEng</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="LeaveType"><DataField>LeaveType</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="LeaveId"><DataField>LeaveId</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="EmpId"><DataField>EmpId</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="LeaveTypeId"><DataField>LeaveTypeId</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="CommissionTypeId"><DataField>CommissionTypeId</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="LeaveDuration"><DataField>LeaveDuration</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="LeaveDurationType"><DataField>LeaveDurationType</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ForeignDuration"><DataField>ForeignDuration</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="ForeignDurationType"><DataField>ForeignDurationType</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TmYear"><DataField>TmYear</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="YearText"><DataField>YearText</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="CreateDate"><DataField>CreateDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="FormDate"><DataField>FormDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="ToDate"><DataField>ToDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="Duration"><DataField>Duration</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="Slot"><DataField>Slot</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="EmpLeaveId"><DataField>EmpLeaveId<
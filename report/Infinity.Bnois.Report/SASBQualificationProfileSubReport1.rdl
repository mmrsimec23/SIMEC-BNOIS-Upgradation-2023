tName><Top>0.0175in</Top><Left>0.01958in</Left><Height>1.7in</Height><Width>1.36459in</Width><Style><Border><Style>None</Style></Border></Style></Tablix></ReportItems><Height>1.7175in</Height><Style /></Body><Width>1.39458in</Width><Page><PageHeight>11.69in</PageHeight><PageWidth>8.27in</PageWidth><LeftMargin>0.1in</LeftMargin><RightMargin>0.1in</RightMargin><TopMargin>0.1in</TopMargin><BottomMargin>0.1in</BottomMargin><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="QualificationProfileDS"><DataSourceReference>BnoisDataSource</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>b508f709-83cb-4dfc-8f3b-432be043aaf5</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="BasicCourseDSet"><Query><DataSourceName>QualificationProfileDS</DataSourceName><QueryParameters><QueryParameter Name="@EmployeeId"><Value>=Parameters!EmployeeId.Value</Value></QueryParameter></QueryParameters><CommandText>

	   select 
case
when CT.CountryId=12 and TR.Positon='' then  CAST(ROUND(TR.Percentage,2) as varchar(20))+'%'
when CT.CountryId&lt;&gt;12  then CT.FullName
else  CAST( ROUND(TR.Percentage,2) as varchar(20))+'%'+ isnull(', '+CAST(TR.Positon as varchar(20))+'/'+cast((select count(TrainingPlanId) from TrainingResult where TrainingPlanId=TR.TrainingPlanId) as varchar(20)),'') 
end as Result ,

ISNULL((SELECT stuff((
select top(2)
 ', ' + cast(crs.FullName as varchar(max)) 
from TrainingResult TR
inner join CourseCategory  CC on TR.CourseCategoryId=CC.CourseCategoryId
inner join TrainingPlan TP on TR.TrainingPlanId=TP.TrainingPlanId
inner join Course crs on TP.CourseId=crs.CourseId
where TR.EmployeeId=315 and crs.SplQualification=1
    FOR XML PATH('')
	   ), 1, 2, '')),'Not done') as SplQualification

from TrainingResult TR
inner join CourseCategory  CC on TR.CourseCategoryId=CC.CourseCategoryId
inner join TrainingPlan TP on TR.TrainingPlanId=TP.TrainingPlanId
inner join Country CT on TP.CountryId=CT.CountryId
where CC.CourseCategoryId=1 and TR.EmployeeId=@EmployeeId</CommandText></Query><Fields><Field Name="Result"><DataField>Result</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="SplQualification"><DataField>SplQualification</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="JSCCourseDSet"><Query><DataSourceName>QualificationProfileDS</DataSourceName><QueryParameters><QueryParameter Name="@EmployeeId"><Value>=Parameters!EmployeeId.Value</Value></QueryParameter></QueryParameters><CommandText>select 
case
when CT.CountryId=12 and TR.Positon=null then CAST(ROUND(TR.Percentage, 2) as varchar(20))+'%'
when CT.CountryId&lt;&gt;12  then CT.FullName
else CAST( ROUND(TR.Percentage,2) as varchar(20))+'%'
+ isnull(', '+CAST(TR.Positon as varchar(20))+'/'+cast((select count(TrainingPlanId) from TrainingResult where TrainingPlanId=TR.TrainingPlanId) as varchar(20)),'') 
end as Result 
from TrainingResult TR
inner join CourseCategory  CC on TR.CourseCategoryId=CC.CourseCategoryId
inner join TrainingPlan TP on TR.TrainingPlanId=TP.TrainingPlanId
inner join Country CT on TP.CountryId=CT.CountryId
inner join ResultType RT on TR.ResultTypeId=RT.ResultTypeId
where CC.CourseCategoryId=2 and TR.EmployeeId=@EmployeeId and RT.ResultTypeId&lt;&gt;1</CommandText></Query><Fields><Field Name="Result"><DataField>Result</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="SpecializationCourseDSet"><Query><DataSourceName>QualificationProfileDS</DataSourceName><QueryParameters><QueryParameter Name="@EmployeeId"><Value>=Parameters!EmployeeId.Value</Value></QueryParameter></QueryParameters><CommandText>select 
case
when CT.CountryId=12 and TR.Positon=NULL then LTRIM(STR(TR.Percentage, 25, 2))+'%'
when CT.CountryId&lt;&gt;12  then CT.FullName
else  CAST(ROUND(TR.Percentage,2) as varchar(20))+'%'+ isnull(', '+CAST(TR.Positon AS varchar(20))+'/'+cast((select count(TrainingPlanId) from TrainingResult where TrainingPlanId=TR.TrainingPlanId) as varchar(20)),'') 
end as Result 
from TrainingResult TR
inner join CourseCategory  CC on TR.CourseCategoryId=CC.CourseCategoryId
inner join TrainingPlan TP on TR.TrainingPlanId=TP.TrainingPlanId
inner join Country CT on TP.CountryId=CT.CountryId
where CC.CourseCategoryId=3 and TR.EmployeeId=@EmployeeId</CommandText></Query><Fields><Field Name="Result"><DataField>Result</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="StaffCourseDSet"><Query><DataSourceName>QualificationProfileDS</DataSourceName><QueryParameters><QueryParameter Name="@EmployeeId"><Value>=Parameters!EmployeeId.Value</Value></QueryParameter></QueryParameters><CommandText>select 
case
when CT.CountryId=12 and TR.Positon=NULL then CAST(ROUND(TR.Percentage,2) AS varchar(20)) +'%'
when CT.CountryId&lt;&gt;12  then CT.FullName
else  CAST(ROUND(TR.Percentage,2) as varchar(20)) +'%'+ isnull(', '+CAST(TR.Positon as varchar(20)) +'/'+cast((select count(TrainingPlanId) from TrainingResult where TrainingPlanId=TR.TrainingPlanId) as varchar(20)),'') 
end as Result
from TrainingResult TR
inner join CourseCategory  CC on TR.CourseCategoryId=CC.CourseCategoryId
inner join TrainingPlan TP on TR.TrainingPlanId=TP.TrainingPlanId
inner join Country CT on TP.CountryId=CT.CountryId
where CC.CourseCategoryId=4 and TR.EmployeeId=@EmployeeId</CommandText></Query><Fields><Field Name="Result"><DataField>Result</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="CommandCourseDSet"><Query><DataSourceName>QualificationProfileDS</DataSourceName><QueryParameters><QueryParameter Name="@EmployeeId"><Value>=Parameters!EmployeeId.Value</Value></QueryParameter></QueryParameters><CommandText>select 
case
when CT.CountryId=12 and (TR.Positon=null or TR.Positon=0) then cast( round(TR.Percentage, 2) as varchar(30))+'%'+', '+  isnull(', '+TR.DsRecom,'')
when CT.CountryId&lt;&gt;12  then CT.FullName+', '+ isnull(TR.DsRecom,'')
else  cast( round(TR.Percentage, 2) as varchar(30))+'%, '+  cast(TR.Positon as varchar(30))+ isnull('/'+cast((select count(TrainingPlanId) from TrainingResult where TrainingPlanId=TR.TrainingPlanId) as varchar(20)),'th') + isnull(', '+TR.DsRecom,'')
end as Result 
from TrainingResult TR
inner join CourseCategory  CC on TR.CourseCategoryId=CC.CourseCategoryId
inner join TrainingPlan TP on TR.TrainingPlanId=TP.TrainingPlanId
inner join Country CT on TP.CountryId=CT.CountryId
where CC.CourseCategoryId=5 and TR.EmployeeId=@EmployeeId</CommandText></Query><Fields><Field Name="Result"><DataField>Result</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="BatchPositionDSet"><Query><DataSourceName>QualificationProfileDS</DataSourceName><QueryParameters><QueryParameter Name="@EmployeeId"><Value>=Parameters!EmployeeId.Value</Value></QueryParameter></QueryParameters><CommandText>select isnull(BatchPosition,0) as BatchPosition, (select count(BatchId) from Employee where BatchId=(select BatchId from Employee where EmployeeId=@EmployeeId)) as  OutOf from Employee
where EmployeeId=@EmployeeId</CommandText></Query><Fields><Field Name="BatchPosition"><DataField>BatchPosition</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="OutOf"><DataField>OutOf</DataField><rd:TypeName>System.Int32</rd:TypeName></Field></Fields></DataSet></DataSets><ReportParameters><ReportParameter Name="EmployeeId"><DataType>Integer</DataType><Prompt>Employee Id</Prompt></ReportParameter></ReportParameters><rd:ReportUnitType>Inch</rd:ReportUnitType><rd:ReportID>06888bf0-1e41-435e-b132-131a5bcb686f</rd:ReportID></Report>
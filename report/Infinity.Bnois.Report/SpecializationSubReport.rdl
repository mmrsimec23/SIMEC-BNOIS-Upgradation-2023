</DataSources><DataSets><DataSet Name="SpecializationDataSet"><Query><DataSourceName>SpecializationDataSource</DataSourceName><QueryParameters><QueryParameter Name="@PNo"><Value>=Parameters!PNo.Value</Value></QueryParameter></QueryParameters><CommandText>select CC.Name as Course,LTRIM(STR(TR.Percentage, 50, 2)) as Percentage,
TR.Positon as Position,C.CountryId, C.FullName as Country,
R.TypeName as ResultType,
case
when C.CountryId=12 and TR.Positon='' then LTRIM(STR(TR.Percentage, 25, 2))
when C.CountryId&lt;&gt;12  then cast(TR.Percentage as varchar(30))+' ('+C.FullName+')'
else  LTRIM(STR(TR.Percentage, 25, 2))+ isnull(', '+TR.Positon,'') +'/'+'('+ cast((select count(TrainingPlanId) from TrainingPlan where TrainingPlanId=TR.TrainingPlanId)as varchar(20))+')'
end as Result
from TrainingResult TR
inner join CourseCategory CC on TR.CourseCategoryId=CC.CourseCategoryId
inner join CourseSubCategory CSC on TR.CourseSubCategoryId=CSC.CourseSubCategoryId
inner join TrainingPlan TP on TR.TrainingPlanId=TP.TrainingPlanId
inner join Country C on C.CountryId=TP.CountryId
inner join ResultType R on TR.ResultTypeId=R.ResultTypeId
where EmployeeId=(select EmployeeId from Employee where PNo=@PNo) and CC.SASB=1 and CC.ShortName='Spl Course'</CommandText></Query><Fields><Field Name="Course"><DataField>Course</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Percentage"><DataField>Percentage</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Position"><DataField>Position</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="CountryId"><DataField>CountryId</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="Country"><DataField>Country</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ResultType"><DataField>ResultType</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Result"><DataField>Result</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet></DataSets><ReportParameters><ReportParameter Name="PNo"><DataType>String</DataType><Prompt>PNo</Prompt></ReportParameter></ReportParameters><rd:ReportUnitType>Inch</rd:ReportUnitType><rd:ReportID>a199cdde-ede3-4de2-9bb5-3053fc5964a0</rd:ReportID></Report>
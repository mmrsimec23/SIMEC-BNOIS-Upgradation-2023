  <DataSets>
    <DataSet Name="PunishmentDSet">
      <Query>
        <DataSourceName>PunishmentDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@EmployeeId">
            <Value>=Parameters!EmployeeId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select (case when pa.PunishmentSubCategoryId=11 then pc.ShortName + ' from ' + psc.ShortName + ' on ' + convert(varchar(11), pa.Date, 106) else (pc.ShortName +' '+ convert(varchar(11),pa.DurationMonths) + ' Month(s) on '+ convert(varchar(11), pa.Date, 106)) end) as punishment,
(case when pc.ShortName='' then '' else '* Recom def ....... yrs' end) as defyr  from PunishmentAccident pa
left join PunishmentCategory pc on pa.PunishmentCategoryId=pc.PunishmentCategoryId
left join PunishmentSubCategory psc on psc.PunishmentSubCategoryId=pa.PunishmentSubCategoryId
where pa.EmployeeId=@EmployeeId  and pa.PunishmentSubCategoryId in (11,32,33,34,35,36,37,38,39,40,43) 
order by [Date]
</CommandText>
      </Query>
      <Fields>
        <Field Name="punishment">
          <DataField>punishment</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="defyr">
          <DataField>defyr</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>

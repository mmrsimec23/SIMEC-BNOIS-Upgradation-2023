GOT COMMENDATION' when Achievement.Type =2 then 'OFFICER GOT APPREACIATION' when Achievement.Type=3 then 'OFFICER GOT NOTABLE ACHIEVEMENT' END ACHIEVEMENT,
Employee.RankId,Date,Employee.FullNameEng,Commendation.Name CommendationName,Remarks from Achievement
left join Employee on Achievement.EmployeeId = Employee.EmployeeId
left join EmployeeGeneral on Achievement.EmployeeId = EmployeeGeneral.EmployeeId
left join Branch on EmployeeGeneral.BranchId = Branch.BranchId
left join Commendation on Achievement.CommendationId = Commendation.CommendationId
left join vwTransfer on Achievement.TransferId =vwTransfer.TransferId
where Employee.PNo = case when @PNo ='' then Employee.PNo else @PNo end  and (Date &gt;= case when @DateFrom is null then Date else @DateFrom  end and Date &lt;= case when @DateTo is null then Date else @DateTo  end)</CommandText><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query><Fields><Field Name="PNo"><DataField>PNo</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Type"><DataField>Type</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="Appointment"><DataField>Appointment</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="BranchName"><DataField>BranchName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="ACHIEVEMENT"><DataField>ACHIEVEMENT</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="RankId"><DataField>RankId</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="Date"><DataField>Date</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.DateTime</rd:TypeName></Field><Field Name="FullNameEng"><DataField>FullNameEng</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="CommendationName"><DataField>CommendationName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Remarks"><Data
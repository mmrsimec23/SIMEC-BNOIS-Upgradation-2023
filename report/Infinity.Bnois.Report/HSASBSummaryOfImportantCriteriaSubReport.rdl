  <DataSets>
    <DataSet Name="CommonDSet">
      <Query>
        <DataSourceName>SummaryOfImportantCriteriaDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@EmployeeId">
            <Value>=Parameters!EmployeeId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        (SELECT        format(AVG(OprGrade), 'N2') AS Expr1
                          FROM            EmployeeOpr
                          WHERE        (EmployeeId = @EmployeeId)) AS AvgOPR, ISNULL
                             ((SELECT        dbo.fnGetSeaServiceByEmployeeId(@EmployeeId) AS Expr1), 'Nil') AS TotalSeaSvc, ISNULL
                             ((SELECT        dbo.fnGetCommandServiceDurationByEmployeeId(@EmployeeId) AS Expr2), 'Nil') AS TotalCommandSvc, ISNULL
                             ((SELECT        dbo.fnGetCommandServiceDurationForSurveyShipByEmployeeId(@EmployeeId) AS Expr3), 'Nil') AS SurveyShipSvc, ISNULL
                             ((SELECT        TOP (1) s.SuitabilityName + ' (' + oas.Note + ')' AS RecomForXOFF
                                 FROM            OprAptSuitability AS oas INNER JOIN
                                                          EmployeeOpr AS eo ON oas.EmployeeOprId = eo.Id INNER JOIN
                                                          Suitability AS s ON oas.SuitabilityId = s.Id INNER JOIN
                                                          SpecialAptType AS sat ON oas.SpecialAptTypeId = sat.Id
                                 WHERE        (eo.EmployeeId = @EmployeeId) AND (sat.SpAptType = 'XO FF')
                                 ORDER BY oas.Id), 'Nil') AS RecomForXOFF</CommandText>
      </Query>
      <Fields>
        <Field Name="AvgOPR">
          <DataField>AvgOPR</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TotalSeaSvc">
          <DataField>TotalSeaSvc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TotalCommandSvc">
          <DataField>TotalCommandSvc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SurveyShipSvc">
          <DataField>SurveyShipSvc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RecomForXOFF">
          <DataField>RecomForXOFF</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="StaffCourseDSet">
      <Query>
        <DataSourceName>SummaryOfImportantCriteriaDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@EmployeeId">
            <Value>=Parameters!EmployeeId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
case
when CT.CountryId=12 and TR.Positon=NULL then CAST(ROUND(TR.Percentage,2) AS varchar(20)) +'%'
when CT.CountryId&lt;&gt;12  then CT.FullName
else  CAST(ROUND(TR.Percentage,2) as varchar(20)) +'%'+ isnull(', '+CAST(TR.Positon as varchar(20)) +'/'+cast((select count(TrainingPlanId) from TrainingResult where TrainingPlanId=TR.TrainingPlanId) as varchar(20)),'') 
end as Result
from TrainingResult TR
inner join CourseCategory  CC on TR.CourseCategoryId=CC.CourseCategoryId
inner join TrainingPlan TP on TR.TrainingPlanId=TP.TrainingPlanId
inner join Country CT on TP.CountryId=CT.CountryId
where CC.CourseCategoryId=4 and TR.EmployeeId=@EmployeeId</CommandText>
      </Query>
      <Fields>
        <Field Name="Result">
          <DataField>Result</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>

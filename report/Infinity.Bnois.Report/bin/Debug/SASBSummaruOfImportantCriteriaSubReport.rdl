<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="SummaryOfImportantCriteriaDS"><DataSourceReference>BnoisDataSource</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>9d2dd072-0462-4a09-9dae-39beb3b3a957</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="CommonDSet"><Query><DataSourceName>SummaryOfImportantCriteriaDS</DataSourceName><QueryParameters><QueryParameter Name="@EmployeeId"><Value>=Parameters!EmployeeId.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT        (SELECT        format(AVG(OprGrade), 'N2') AS Expr1
                          FROM            EmployeeOpr
                          WHERE        (EmployeeId = @EmployeeId)) AS AvgOPR, ISNULL
                             ((SELECT        dbo.fnGetSeaServiceByEmployeeId(@EmployeeId) AS Expr1), 'Nil') AS TotalSeaSvc, ISNULL
                             ((SELECT        dbo.fnGetCommandServiceDurationByEmployeeId(@EmployeeId) AS Expr1), 'Nil') AS TotalCommandSvc, ISNULL
                             ((SELECT        dbo.fnGetCommandServiceDurationForMediumShipByEmployeeId(@EmployeeId) AS Expr1), 'Nil') AS CommandSvcMediumShip, ISNULL
                             ((SELECT        TOP (1) s.SuitabilityName + ' (' + oas.Note + ')' AS RecomForXOFF
                                 FROM            OprAptSuitability AS oas INNER JOIN
                                                          EmployeeOpr AS eo ON oas.EmployeeOprId = eo.Id INNER JOIN
                                                          Suitability AS s ON oas.SuitabilityId = s.Id INNER JOIN
                                                          SpecialAptType AS sat ON oas.SpecialAptTypeId = sat.Id
                                 WHERE        (eo.EmployeeId = @EmployeeId) AND (sat.SpAptType = 'XO FF')
                                 ORDER BY oas.Id), 'Nil') AS RecomForXOFF</CommandText></Query><Fields><Field Name="AvgOPR"><DataField>AvgOPR</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TotalSeaSvc"><DataField>TotalSeaSvc</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TotalCommandSvc"><DataField>TotalCommandSvc</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="CommandSvcMediumShip"><DataField>CommandSvcMediumShip</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="RecomForXOFF"><DataField>RecomForXOFF</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="StaffCourseDSet"><Query><DataSourceName>SummaryOfImportantCriteriaDS</DataSourceName><QueryParameters><QueryParameter Name="@EmployeeId"><Value>=Parameters!EmployeeId.Value</Value></QueryParameter></QueryParameters><CommandText>select 
case
when CT.CountryId=12 and TR.Positon=NULL then CAST(ROUND(TR.Percentage,2) AS varchar(20)) +'%'
when CT.CountryId&lt;&gt;12  then CT.FullName
else  CAST(ROUND(TR.Percentage,2) as varchar(20)) +'%'+ isnull(', '+CAST(TR.Positon as varchar(20)) +'/'+cast((select count(TrainingPlanId) from TrainingResult where TrainingPlanId=TR.TrainingPlanId) as varchar(20)),'') 
end as Result
from TrainingResult TR
inner join CourseCategory  CC on TR.CourseCategoryId=CC.CourseCategoryId
inner join TrainingPlan TP on TR.TrainingPlanId=TP.TrainingPlanId
inner join Country CT on TP.CountryId=CT.CountryId
where CC.CourseCategoryId=4 and TR.EmployeeId=@EmployeeId</CommandText></Query><Fields><Field Name="Result"><DataField>Result</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="MedShipCODSet"><Query><DataSourceName>SummaryOfImportantCriteriaDS</DataSourceName><QueryParameters><QueryParameter Name="@EmployeeId"><Value>=Parameters!EmployeeId.Value</Value></QueryParameter></QueryParameters><CommandType>StoredProcedure</CommandType><CommandText>spGetSASBCoMedShipSvcDetails</CommandText></Query><Fields><Field Name="Office"><DataField>Office</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Appointment"><DataField>Appointment</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Total"><DataField>Total</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet></DataSets><ReportSections><ReportSection><Body><ReportItems><Textbox Name="Textbox1"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>OPR Avg (min 5.8)</Value><Style><FontSize>8pt</FontSize><FontWeight>Bold</FontWeight><TextDecoration>Underline</TextDecoration></Style></TextRun></TextRuns><Style /></Paragraph><Paragraph><TextRuns><TextRun><Label>OprAvg</Label><Value>=First(Fields!AvgOPR.Value, "CommonDSet")</Value><Style><FontSize>8pt</FontSize></Style></TextRun></TextRuns><Style /></Paragraph></Paragraphs><rd:DefaultName>Textbox1</rd:DefaultName><Top>0.025in</Top><Left>0.025in</Left><Height>0.325in</Height><Width>1.63333in</Width><Style><Border><Style>None</Style></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox><Textbox Name="Textbox2"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>Staff Course (min 60%)</Value><Style><FontSize>8pt</FontSize><FontWeight>Bold</FontWeight><TextDecoration>Underline</TextDe
<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="QualificationProfileDS"><DataSourceReference>BnoisDataSource</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>b508f709-83cb-4dfc-8f3b-432be043aaf5</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="BasicCourseDSet"><Query><DataSourceName>QualificationProfileDS</DataSourceName><QueryParameters><QueryParameter Name="@EmployeeId"><Value>=Parameters!EmployeeId.Value</Value></QueryParameter></QueryParameters><CommandText>

	   select 
case
when CT.CountryId=12 and TR.Positon='' then  CAST(ROUND(TR.Percentage,2) as varchar(20))+'%'
when CT.CountryId&lt;&gt;12  then CT.FullName
else  CAST( ROUND(TR.Percentage,2) as varchar(20))+'%'+ isnull(', '+CAST(TR.Positon as varchar(20))+'/'+cast((select count(TrainingPlanId) from TrainingResult where TrainingPlanId=TR.TrainingPlanId) as varchar(20)),'') 
end as Result ,

ISNULL((SELECT stuff((
select top(2)
 ', ' + cast(crs.FullName as varchar(max)) 
from TrainingResult TR
inner join CourseCategory  CC on TR.CourseCategoryId=CC.CourseCategoryId
inner join TrainingPlan TP on TR.TrainingPlanId=TP.TrainingPlanId
inner join Course crs on TP.CourseId=crs.CourseId
where TR.EmployeeId=315 and crs.SplQualification=1
    FOR XML PATH('')
	   ), 1, 2, '')),'Not done') as SplQualification

from TrainingResult TR
inner join CourseCategory  CC on TR.CourseCategoryId=CC.CourseCategoryId
inner join TrainingPlan TP on TR.TrainingPlanId=TP.TrainingPlanId
inner join Country CT on TP.CountryId=CT.CountryId
where CC.CourseCategoryId=1 and TR.EmployeeId=@EmployeeId</CommandText></Query><Fields><Field Name="Result"><DataField>Result</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="SplQualification"><DataField>SplQualification</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="JSCCourseDSet"><Query><DataSourceName>QualificationProfileDS</DataSourceName><QueryParameters><QueryParameter Name="@EmployeeId"><Value>=Parameters!EmployeeId.Value</Value></QueryParameter></QueryParameters><CommandText>select 
case
when CT.CountryId=12 and TR.Positon=null then CAST(ROUND(TR.Percentage, 2) as varchar(20))+'%'
when CT.CountryId&lt;&gt;12  then CT.FullName
else CAST( ROUND(TR.Percentage,2) as varchar(20))+'%'
+ isnull(', '+CAST(TR.Positon as varchar(20))+'/'+cast((select count(TrainingPlanId) from TrainingResult where TrainingPlanId=TR.TrainingPlanId) as varchar(20)),'') 
end as Result 
from TrainingResult TR
inner join CourseCategory  CC on TR.CourseCategoryId=CC.CourseCategoryId
inner join TrainingPlan TP on TR.TrainingPlanId=TP.TrainingPlanId
inner join Country CT on TP.CountryId=CT.CountryId
inner join ResultType RT on TR.ResultTypeId=RT.ResultTypeId
where CC.CourseCategoryId=2 and TR.EmployeeId=@EmployeeId and RT.ResultTypeId&lt;&gt;1</CommandText></Query><Fields><Field Name="Result"><DataField>Result</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="SpecializationCourseDSet"><Query><DataSourceName>QualificationProfileDS</DataSourceName><QueryParameters><QueryParameter Name="@EmployeeId"><Value>=Parameters!EmployeeId.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT        CASE WHEN CT.CountryId = 12 AND TR.Positon = NULL THEN LTRIM(STR(TR.Percentage, 25, 2)) + '%' WHEN CT.CountryId &lt;&gt; 12 THEN CT.FullName ELSE CAST(ROUND(TR.Percentage, 2) AS varchar(20)) 
                         + '%' + isnull(', ' + CAST(TR.Positon AS varchar(20)) + '/' + CAST
                             ((SELECT        COUNT(TrainingPlanId)
                                 FROM            TrainingResult
                                 WHERE        TrainingPlanId = TR.TrainingPlanId) AS varchar(20)), '') END AS Result
FROM            TrainingResult AS TR INNER JOIN
                         CourseCategory AS CC ON TR.CourseCategoryId = CC.CourseCategoryId INNER JOIN
                         TrainingPlan AS TP ON TR.TrainingPlanId = TP.TrainingPlanId INNER JOIN
                         Country AS CT ON TP.CountryId = CT.CountryId
WHERE        (CC.CourseCategoryId = 3) AND (TR.EmployeeId = @EmployeeId)</CommandText></Query><Fields><Field Name="Result"><DataField>Result</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="StaffCourseDSet"><Query><DataSourceName>QualificationProfileDS</DataSourceName><QueryParameters><QueryParameter Name="@EmployeeId"><Value>=Parameters!EmployeeId.Value</Value></QueryParameter></QueryParameters><CommandText>select 
case
when CT.CountryId=12 and TR.Positon=NULL then CAST(ROUND(TR.Percentage,2) AS varchar(20)) +'%'
when CT.CountryId&lt;&gt;12  then CT.FullName
else  CAST(ROUND(TR.Percentage,2) as varchar(20)) +'%'+ isnull(', '+CAST(TR.Positon as varchar(20)) +'/'+cast((select count(TrainingPlanId) from TrainingResult where TrainingPlanId=TR.TrainingPlanId) as varchar(20)),'') 
end as Result
from TrainingResult TR
inner join CourseCategory  CC on TR.CourseCategoryId=CC.CourseCategoryId
inner join TrainingPlan TP on TR.TrainingPlanId=TP.TrainingPlanId
inner join Country CT on TP.CountryId=CT.CountryId
where CC.CourseCategoryId=4 and TR.EmployeeId=@EmployeeId</CommandText></Query><Fields><Field Name="Result"><DataField>Result</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="CommandCourseDSet"><Query><DataSourceName>QualificationProfileDS</DataSourceName><QueryParameters><QueryParameter Name="@EmployeeId"><Value>=Parameters!EmployeeId.Value</Value></QueryParameter></QueryParameters><CommandText>select 
case
when CT.CountryId=12 and (TR.Positon=null or TR.Positon=0) then cast( round(TR.Percentage, 2) as varchar(30))+'%'+', '+  isnull(', '+TR.DsRecom,'')
when CT.CountryId&lt;&gt;12  then CT.FullName+', '+ isnull(TR.DsRecom,'')
else  cast( round(TR.Percentage, 2) as varchar(30))+'% ' + isnull(', '+TR.DsRecom,'')
end as Result 
from TrainingResult TR
inner join CourseCategory  CC on TR.CourseCategoryId=CC.CourseCategoryId
inner join TrainingPlan TP on TR.TrainingPlanId=TP.TrainingPlanId
inner join Country CT on TP.CountryId=CT.CountryId
where CC.CourseCategoryId=5 and TR.EmployeeId=@EmployeeId</CommandText></Query><Fields><Field Name="Result"><DataField>Result</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="HigherCmdCourseDSet"><Query><DataSourceName>QualificationProfileDS</DataSourceName><QueryParameters><QueryParameter Name="@EmployeeId"><Value>=Parameters!EmployeeId.Value</Value></QueryParameter></QueryParameters><CommandText>select
case when  (select count(CC.name)  
from TrainingResult TR
inner join CourseCategory  CC on TR.CourseCategoryId=CC.CourseCategoryId
inner join TrainingPlan TP on TR.TrainingPlanId=TP.TrainingPlanId
inner join Country CT on TP.CountryId=CT.CountryId
where CC.CourseCategoryId=25 and TR.EmployeeId=@EmployeeId) =1 then 'Done'
else 'Not Done' end as Result</CommandText></Query><Fields><Field Name="Result"><DataField>Result</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="BatchPositionDSet"><Query><DataSourceName>QualificationProfileDS</DataSourceName><QueryParameters><QueryParameter Name="@EmployeeId"><Value>=Parameters!EmployeeId.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT        ISNULL(BatchPosition, 0) AS BatchPosition,
                             (SELECT        COUNT(BatchId) AS Expr1
                               FROM            Employee
                               WHERE        (BatchId = Employee_1.BatchId) AND (EmployeeStatusId = 1)) AS OutOf
FROM            Employee AS Employee_1
WHERE        (EmployeeId = @EmployeeId)</CommandText></Query><Fields><Field Name="BatchPosition"><DataField>BatchPosition</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="OutOf"><DataField>OutOf</DataField><rd:TypeName>System.Int32</rd:TypeName></Field></Fields></DataSet><DataSet Name="OfficerBasicInfo"><Query><DataSourceName>QualificationProfileDS</DataSourceName><QueryParameters><QueryParameter Name="@EmployeeId"><Value>=Parameters!EmployeeId.Value</Value></QueryParameter></QueryParameters><CommandText>select e.EmployeeId, eg.BranchId, e.RankId from Employee e
